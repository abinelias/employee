
<div class = "textfont">
	<div style="text-align:left;">
		<table>
			<tr>
				<td>Company *</td>
				<td><input type="text" name="company"  id="company" ></td>
			</tr>
			<tr>
				<td>Position *</td>
				<td><input type="text"  value="" name="job" id="job"></td>
			</tr>
			<tr>
				<td>From *</td>
				<td><input type="text" class="sdate" name="from" id="opdate"> </td>
			</tr>
			<tr>
				<td>Until *</td>
				<td><input type="text" name="till" id="datepickers"> </td>
			</tr>
			<tr>
				<td colspan="2">
					<button class="buttonStyle" type="button">Add</button>
					<%= submit_tag "Cancel", :onclick => "window.close()" , :class => 'buttonStyle' %>
				 </td>
			</tr>
		</table>
	</div>
</div>


